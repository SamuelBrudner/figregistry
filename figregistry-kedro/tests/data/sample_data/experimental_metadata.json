            "legend.fontsize": 9
          },
          "color_palette": "Set1",
          "style_modifiers": ["draft_quality", "speed_optimized"],
          "file_format_preferences": ["png"],
          "compression_level": "standard"
        },
        "presentation": {
          "rcParams": {
            "figure.figsize": [10, 7],
            "figure.dpi": 150,
            "axes.grid": false,
            "font.size": 12,
            "axes.titlesize": 14,
            "axes.labelsize": 12,
            "legend.fontsize": 11,
            "axes.spines.top": false,
            "axes.spines.right": false
          },
          "color_palette": "professional",
          "style_modifiers": ["clean_aesthetics", "high_contrast"],
          "file_format_preferences": ["png", "pdf"],
          "compression_level": "high"
        },
        "publication": {
          "rcParams": {
            "figure.figsize": [6, 4],
            "figure.dpi": 300,
            "axes.grid": false,
            "font.family": "serif",
            "font.size": 8,
            "axes.titlesize": 10,
            "axes.labelsize": 8,
            "legend.fontsize": 7,
            "axes.linewidth": 0.5,
            "lines.linewidth": 1.0
          },
          "color_palette": "academic_grayscale",
          "style_modifiers": ["publication_quality", "space_efficient"],
          "file_format_preferences": ["pdf", "svg", "eps"],
          "compression_level": "lossless"
        }
      },
      "condition_specific_styles": {
        "control": {
          "color": "#1f77b4",
          "marker": "o",
          "linestyle": "-",
          "alpha": 0.8,
          "style_priority": "baseline"
        },
        "treatment_a": {
          "color": "#ff7f0e",
          "marker": "s",
          "linestyle": "--",
          "alpha": 0.8,
          "style_priority": "highlight"
        },
        "treatment_b": {
          "color": "#2ca02c",
          "marker": "^",
          "linestyle": "-.",
          "alpha": 0.8,
          "style_priority": "comparison"
        },
        "experimental": {
          "color": "#d62728",
          "marker": "D",
          "linestyle": ":",
          "alpha": 0.9,
          "style_priority": "emphasis"
        }
      },
      "hierarchical_style_inheritance": {
        "_description": "Demonstrates hierarchical style merging for nested conditions",
        "base_style": {
          "figure.facecolor": "white",
          "axes.facecolor": "white",
          "axes.edgecolor": "black",
          "axes.linewidth": 1.0
        },
        "treatment_modifiers": {
          "treatment_a": {
            "axes.prop_cycle": "cycler('color', ['#ff7f0e', '#ffbb78'])"
          },
          "treatment_b": {
            "axes.prop_cycle": "cycler('color', ['#2ca02c', '#98df8a'])"
          }
        },
        "quality_modifiers": {
          "high_quality": {
            "lines.markersize": 6,
            "lines.markeredgewidth": 1.0
          },
          "medium_quality": {
            "lines.markersize": 4,
            "lines.markeredgewidth": 0.5,
            "axes.alpha": 0.9
          },
          "low_quality": {
            "lines.markersize": 3,
            "lines.markeredgewidth": 0.2,
            "axes.alpha": 0.7,
            "text.alpha": 0.8
          }
        }
      },
      "fallback_mechanisms": {
        "_description": "Fallback strategies for undefined or conflicting conditions",
        "default_fallback": {
          "style_name": "safe_default",
          "rcParams": {
            "figure.figsize": [8, 6],
            "figure.dpi": 100,
            "font.size": 10
          },
          "color": "#333333",
          "trigger_conditions": ["undefined", "null", "unknown"]
        },
        "error_recovery": {
          "style_name": "error_safe",
          "rcParams": {
            "figure.figsize": [6, 4],
            "figure.dpi": 72,
            "font.size": 8
          },
          "color": "#ff0000",
          "trigger_conditions": ["error", "invalid", "malformed"]
        }
      }
    }
  },
