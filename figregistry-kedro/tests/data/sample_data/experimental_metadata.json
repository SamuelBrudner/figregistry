  "figregistry_styling_patterns": {
    "_description": "Condition mapping definitions aligned with FigRegistry styling patterns for testing automated style application per F-002",
    "condition_style_mappings": {
      "purpose_based_styles": {
        "exploratory": {
          "rcParams": {
            "figure.figsize": [8, 6],
            "figure.dpi": 100,
            "axes.grid": true,
            "axes.grid.alpha": 0.3,
            "font.size": 10,
            "axes.titlesize": 12,
