      "nested_condition_hierarchies": {
        "_description": "Complex nested conditions for testing hierarchical condition resolution per F-005.4",
        "experimental_design_matrix": {
          "factorial_2x3": {
            "condition_combination_id": "FACT-2x3-001",
            "primary_factor": {
              "factor_name": "treatment_type",
              "levels": ["control", "treatment_a"],
              "factor_type": "categorical"
            },
            "secondary_factor": {
              "factor_name": "measurement_period",
              "levels": ["baseline", "mid_treatment", "post_treatment"],
              "factor_type": "ordinal"
            },
            "condition_combinations": [
              {
                "combination_id": "control_baseline",
                "condition_params": {
                  "treatment": "control",
                  "period": "baseline",
                  "style_override": "control_baseline_style"
                },
                "expected_style_resolution": "control_style + baseline_modifiers"
              },
              {
                "combination_id": "control_mid",
                "condition_params": {
                  "treatment": "control",
                  "period": "mid_treatment",
                  "style_override": "control_progress_style"
                },
                "expected_style_resolution": "control_style + progress_modifiers"
              },
              {
                "combination_id": "treatment_a_post",
                "condition_params": {
                  "treatment": "treatment_a",
                  "period": "post_treatment",
                  "style_override": "treatment_final_style"
                },
                "expected_style_resolution": "treatment_style + final_modifiers"
              }
            ]
          }
        },
        "wildcard_patterns": {
          "_description": "Patterns for testing wildcard matching in condition resolution",
          "treatment_wildcard": {
            "pattern": "treatment_*",
            "description": "Matches any treatment condition",
            "test_cases": [
              "treatment_a",
              "treatment_b",
              "treatment_experimental",
              "treatment_control_variant"
            ],
            "expected_matches": 4,
            "fallback_style": "generic_treatment_style"
          },
          "phase_wildcard": {
            "pattern": "*_phase",
            "description": "Matches any phase-related condition",
            "test_cases": [
              "pilot_phase",
              "main_phase",
              "validation_phase",
              "cleanup_phase"
            ],
            "expected_matches": 4,
            "fallback_style": "generic_phase_style"
          },
          "quality_wildcard": {
            "pattern": "quality_*_data",
            "description": "Matches quality-level data conditions",
            "test_cases": [
              "quality_high_data",
              "quality_medium_data",
              "quality_low_data",
              "quality_unknown_data"
            ],
            "expected_matches": 4,
            "fallback_style": "quality_aware_style"
          }
        }
      }
    }
