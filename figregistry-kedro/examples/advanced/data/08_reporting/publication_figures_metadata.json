  "training_pipeline_figures": {
    "data_quality_analysis": {
      "catalog_reference": "data_quality_analysis_plot",
      "figure_specification": {
        "type": "figregistry_kedro.datasets.FigureDataSet",
        "filepath": "data/08_reporting/figures/training/data_quality_analysis.png",
        "purpose": "exploratory",
        "condition_param": "data_quality_condition",
        "description": "Comprehensive data quality analysis visualization with automated quality condition styling"
      },
      "publication_metadata": {
        "title": "Data Quality Assessment and Validation",
        "subtitle": "Automated quality indicator analysis with condition-based styling",
        "author": "Advanced ML Pipeline Team",
        "publication_context": "exploratory_analysis",
        "target_audience": "data_engineering_team",
        "quality_level": "development_grade",
        "statistical_rigor": "descriptive_analysis"
      },
      "styling_configuration": {
        "condition_resolution": {
          "parameter_source": "data_quality_condition",
          "resolution_pattern": "training.data.{condition_value}",
          "fallback_condition": "training.data.raw",
          "environment_suffix": true
        },
        "style_overrides": {
          "alpha": 0.9,
          "grid_alpha": 0.8,
          "annotation_enabled": true,
          "quality_indicators": true,
          "color_intensity": 0.8
        },
        "conditional_styling": {
          "high_quality": {
            "condition": "data_quality_score >= 0.85",
            "style_adjustments": {
              "color": "#27AE60",
              "linewidth": 3.0,
              "marker": "o",
              "alpha": 1.0
            }
          },
          "medium_quality": {
            "condition": "data_quality_score >= 0.70 and data_quality_score < 0.85",
            "style_adjustments": {
              "color": "#F39C12",
              "linewidth": 2.5,
              "marker": "s",
              "alpha": 0.8
            }
          },
          "low_quality": {
            "condition": "data_quality_score < 0.70",
            "style_adjustments": {
              "color": "#E74C3C",
              "linewidth": 2.0,
              "marker": "x",
              "alpha": 0.6
            }
          }
        }
      },
      "format_configuration": {
        "development": {
          "dpi": 150,
          "bbox_inches": "tight",
          "facecolor": "white",
          "edgecolor": "none",
          "format": "png"
        },
        "staging": {
          "dpi": 200,
          "bbox_inches": "tight",
          "facecolor": "white",
          "transparent": false,
          "format": "png"
        },
        "production": {
          "dpi": 300,
          "bbox_inches": "tight",
          "facecolor": "white",
          "transparent": false,
          "format": "png"
        }
      },
      "context_injection": {
        "parameter_mapping": {
          "data_quality_condition": "parameters.data_processing.data_quality.quality_condition",
          "data_quality_score": "parameters.data_processing.data_quality.quality_score",
          "environment": "run_environment"
        },
        "dynamic_resolution": true,
        "cache_enabled": true
      }
    },

