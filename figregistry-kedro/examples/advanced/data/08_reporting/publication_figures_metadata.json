          "publication_overrides": {
            "figure.figsize": [12, 8],
            "figure.dpi": 150,
            "axes.titlesize": 16,
            "axes.labelsize": 14,
            "legend.fontsize": 12,
            "grid.alpha": 0.8,
            "grid.linewidth": 0.8
          },
          "condition_specific_styling": {
            "training.data.raw": {
              "color": "#E74C3C",
              "marker": "o",
              "linestyle": ":",
              "alpha": 0.7,
              "label": "Raw Data Quality"
            },
            "training.data.cleaned": {
              "color": "#F39C12",
              "marker": "s", 
              "linestyle": "--",
              "alpha": 0.8,
              "label": "Cleaned Data Quality"
            },
            "training.data.transformed": {
              "color": "#27AE60",
              "marker": "^",
              "linestyle": "-",
              "alpha": 1.0,
              "label": "Transformed Data Quality"
            }
          }
