  "description": "Publication-ready figure metadata catalog demonstrating sophisticated FigRegistry-Kedro integration with advanced styling, condition-based parameter resolution, and multi-environment configuration support.",
  "purpose": "advanced_figregistry_integration_showcase",
  "figregistry_compatibility": {
    "minimum_version": "0.3.0",
    "kedro_compatibility": ">=0.18.0,<0.20.0",
    "features_required": [
      "condition_based_styling",
      "advanced_parameter_resolution",
      "environment_specific_configuration",
      "hierarchical_condition_matching",
      "purpose_based_classification"
    ]
